{
  "menuItems": [
    {
      "description": "Datadog dashboard filtered by app label",
      "title": "Datadog App Dashboard",
      "url": "https://app.datadoghq.com/dashboard/abc-123",
      "filters": {
        "mustHaveLabels": [
          { "key": "app" }
        ]
      },
      "templateVars": [
        { "label": "app", "urlAppend": "?tpl_var_app=$LABEL_VALUE" }
      ]
    },
    {
      "description": "Only shows for pods with app=nginx and env=production",
      "title": "Prod Nginx Logs",
      "url": "https://grafana.example.com/nginx",
      "filters": {
        "mustHaveLabels": [
          { "key": "app", "value": "nginx" },
          { "key": "env", "value": "production" }
        ]
      }
    },
    {
      "description": "Only shows for pods with the 'app' label (any value)",
      "title": "App Dashboard",
      "url": "https://grafana.example.com/apps",
      "filters": {
        "mustHaveLabels": [
          { "key": "app" }
        ]
      }
    },
